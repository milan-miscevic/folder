{
    "name": "milan-miscevic/folder",
    "description": "Folder scanner",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "absolute-path",
        "active-folder",
        "files",
        "folder",
        "folders",
        "relative-path",
        "scanner"
    ],
    "authors": [
        {
            "name": "Milan Miščević",
            "email": "milan.miscevic@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8.0",
        "infection/infection": "^0.26.6",
        "phpstan/phpstan": "^1.5.2",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^9.5.19",
        "vimeo/psalm": "^4.22.0"
    },
    "autoload": {
        "psr-4": {
            "Mmm\\Folder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmm\\Folder\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {},
        "sort-packages": true
    }
}
